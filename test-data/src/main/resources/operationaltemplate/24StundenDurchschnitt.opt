        <term_definitions code="at0000">
          <items id="comment">Häufig wird der Ausdruck 'Blutdruck' zur Bezeichung der Messung des brachialen Ateriendrucks im Oberarm verwendet.</items>
          <items id="description">Die lokale Messung des arteriellen Blutdrucks als Surrogat für den arteriellen Druck in der systemischen Zirkulation.</items>
          <items id="text">Blutdruck</items>
        </term_definitions>
        <term_definitions code="at0001">
          <items id="description">Historie</items>
          <items id="text">Historie</items>
        </term_definitions>
        <term_definitions code="at0003">
          <items id="description">*@ internal @(en)</items>
          <items id="text">Blutdruck</items>
        </term_definitions>
        <term_definitions code="at0004">
          <items id="description">Der höchste arterielle Blutdruck eines Zyklus - gemessen in der systolischen oder Kontraktionsphase des Herzens.</items>
          <items id="text">Systolisch</items>
        </term_definitions>
        <term_definitions code="at0005">
          <items id="description">Der minimale systemische arterielle Blutdruck eines Zyklus - gemessen in der diastolischen oder Entspannungsphase des Herzens.</items>
          <items id="text">Diastolisch</items>
        </term_definitions>
        <term_definitions code="at0006">
          <items id="description">*Default, unspecified point in time or interval event which may be explicitly defined in a template or at run-time.(en)</items>
          <items id="text">*Any event(en)</items>
        </term_definitions>
        <term_definitions code="at0007">
          <items id="description">*@ internal @(en)</items>
          <items id="text">*state structure(en)</items>
        </term_definitions>
        <term_definitions code="at0008">
          <items id="description">Die Position der untersuchten Person während der Messung.</items>
          <items id="text">Position</items>
        </term_definitions>
        <term_definitions code="at0011">
          <items id="description">Listenstruktur</items>
          <items id="text">Listenstruktur</items>
        </term_definitions>
        <term_definitions code="at0013">
          <items id="comment">Perloff D, Grim C, Flack J, Frohlich ED, Hill M, McDonald M, Morgenstern BZ. Human blood pressure determination by sphygmomanometry. Circulation 1993;88;2460-2470.</items>
          <items id="description">Die Größe der Manschette, die zur Blutdruckmessung benutzt wurde.</items>
          <items id="text">Manschettengröße</items>
        </term_definitions>
        <term_definitions code="at0014">
          <items id="description">Einfache Körperstelle an der der Blutdruck gemessen wurde.</items>
          <items id="text">Stelle der Messung</items>
        </term_definitions>
        <term_definitions code="at0015">
          <items id="description">Eine Manschette für den Oberschenkel eines Erwachsenen - ca. 20cm x 42cm.</items>
          <items id="text">Oberschenkel eines Erwachsenen</items>
        </term_definitions>
        <term_definitions code="at0016">
          <items id="description">Eine Manschette für Erwachsene mit größeren Armen - ca. 16cm x 38cm.</items>
          <items id="text">Großer Erwachsener</items>
        </term_definitions>
        <term_definitions code="at0017">
          <items id="description">Eine Standard-Manschette für einen Erwachsenen - ca. 13cm x 30cm.</items>
          <items id="text">Erwachsener</items>
        </term_definitions>
        <term_definitions code="at0025">
          <items id="description">Der rechte Arm der Person.</items>
          <items id="text">Rechter Arm</items>
        </term_definitions>
        <term_definitions code="at0026">
          <items id="description">Der linke Arm der Person.</items>
          <items id="text">Linker Arm</items>
        </term_definitions>
        <term_definitions code="at0027">
          <items id="description">Der rechte Oberschenkel der Person.</items>
          <items id="text">Rechter Oberschenkel</items>
        </term_definitions>
        <term_definitions code="at0028">
          <items id="description">Der linke Oberschenkel der Person.</items>
          <items id="text">Linker Oberschenkel</items>
        </term_definitions>
        <term_definitions code="at0033">
          <items id="description">Zusätzlicher Freitext über die Messung, der nicht in anderen Felder erfasst werden konnte.</items>
          <items id="text">Kommentar</items>
        </term_definitions>
        <term_definitions code="at1000">
          <items id="description">Stehend zum Zeitpunkt der Blutdruckmessung.</items>
          <items id="text">Stehend</items>
        </term_definitions>
        <term_definitions code="at1001">
          <items id="description">Sitzend zum Zeitpunkt der Blutdruckmessung (z.B. auf einem Bett oder Stuhl).</items>
          <items id="text">Sitzend</items>
        </term_definitions>
        <term_definitions code="at1002">
          <items id="description">Patient zurückgelehnt zum Zeitpunkt der Blutdruckmessung.</items>
          <items id="text">Zurückgelehnt</items>
        </term_definitions>
        <term_definitions code="at1003">
          <items id="description">Flach liegend zum Zeitpunkt der Blutdruckmessung.</items>
          <items id="text">Liegend</items>
        </term_definitions>
        <term_definitions code="at1005">
          <items id="description">Die kraniokaudale Neigung der Oberfläche auf der die Person zum Zeitpunkt der Messung liegt.</items>
          <items id="text">Neigung</items>
        </term_definitions>
        <term_definitions code="at1006">
          <items id="description">Der mittlerer arterielle Druck über den gesamten Verlauf der Konktraktions- und Entspannungsphase des Herzens.</items>
          <items id="text">Mittlerer arterieller Druck</items>
        </term_definitions>
        <term_definitions code="at1007">
          <items id="description">Der Abstand zwischen dem systolischen und dem diastolischen Blutdruckwert.</items>
          <items id="text">Pulsdruck</items>
        </term_definitions>
        <term_definitions code="at1008">
          <items id="description">Eine Manschette für einen kleinen Erwachsenen - ca. 10cm x 24cm.</items>
          <items id="text">Kleiner Erwachsener</items>
        </term_definitions>
        <term_definitions code="at1009">
          <items id="description">Eine Manschette für ein Kind oder auch einen Erwachsenen mit einem schmalen Arm - 8cm x 21cm.</items>
          <items id="text">Pädiatrisch/Kind</items>
        </term_definitions>
        <term_definitions code="at1010">
          <items id="description">Dient der Dokumentation des Korotkoff-Geräusches, das verwendet wird, um bei auskultarischer Messung den diastolischen Blutdruck zu bestimmen.</items>
          <items id="text">Diastolischer Endpunkt</items>
        </term_definitions>
        <term_definitions code="at1011">
          <items id="description">Das 4. Korotkoff-Geräusch - Die Geräusche klingen plätzlich gedämpft.</items>
          <items id="text">Phase IV</items>
        </term_definitions>
        <term_definitions code="at1012">
          <items id="description">Das 5. Korotkoff-Geräusch - die Geräusche verschwinden völlig während der Manschettendruck unter den diastolischen Blutdruck fällt.</items>
          <items id="text">Phase V</items>
        </term_definitions>
        <term_definitions code="at1014">
          <items id="description">Flach liegend mit seitlicher Neigung, normalerweise zur linken Seite. Häufig verwendet im letzten Drittel eine Schwangerschaft, um aorto-cavale Kompression zu vermeiden.</items>
          <items id="text">Nach links geneigt liegend</items>
        </term_definitions>
        <term_definitions code="at1018">
          <items id="description">Eine Manschette für Kleinkinder und Säuglinge - ca. 5cm x 15cm.</items>
          <items id="text">Kleinkind</items>
        </term_definitions>
        <term_definitions code="at1019">
          <items id="description">Eine Manschette für Neugeborene mit passender Größe für die Reife und das Geburtsgewicht des Neugeborenen.</items>
          <items id="text">Neonatal</items>
        </term_definitions>
        <term_definitions code="at1020">
          <items id="description">Das rechte Handgelenk der Person.</items>
          <items id="text">Rechtes Handgelenk</items>
        </term_definitions>
        <term_definitions code="at1021">
          <items id="description">Das linke Handgelenk der Person.</items>
          <items id="text">Linkes Handgelenk</items>
        </term_definitions>
        <term_definitions code="at1025">
          <items id="description">Details über das Sphygmomanometer oder ein anderes Gerät, dass zur Blutdruckmessung verwendet wird.</items>
          <items id="text">Gerät</items>
        </term_definitions>
        <term_definitions code="at1026">
          <items id="description">Das rechte Fußgelenk der Person.</items>
          <items id="text">Rechtes Fußgelenk</items>
        </term_definitions>
        <term_definitions code="at1030">
          <items id="description">Details über physische Aktivitäten zur Zeit der Blutdruckmessung.</items>
          <items id="text">Anstrengung</items>
        </term_definitions>
        <term_definitions code="at1031">
          <items id="description">Das linke Fußgelenk der Person</items>
          <items id="text">Linkes Fußgelenk</items>
        </term_definitions>
        <term_definitions code="at1032">
          <items id="description">Ein Finger des Individuums.</items>
          <items id="text">Finger</items>
        </term_definitions>
        <term_definitions code="at1035">
          <items id="description">Methode der Messung des Blutdrucks.</items>
          <items id="text">Methode</items>
        </term_definitions>
        <term_definitions code="at1036">
          <items id="description">Auskulatorische Messung unter Benutzung eines Stethoskops und der Korotkoff-Geräusche.</items>
          <items id="text">Auskultation</items>
        </term_definitions>
        <term_definitions code="at1037">
          <items id="description">Palpatorische Messung, normalerweise an den brachialen oder radialen Arterien.</items>
          <items id="text">Palpation</items>
        </term_definitions>
        <term_definitions code="at1038">
          <items id="description">Die Formel die ggf. verwendet wurde, um den mittleren arteriellen Druck zu berechnen.</items>
          <items id="text">Formel für mittleren arterieller Druck</items>
        </term_definitions>
        <term_definitions code="at1039">
          <items id="description">Messung durch eine Blutdruckmaschine.</items>
          <items id="text">Machine</items>
        </term_definitions>
        <term_definitions code="at1040">
          <items id="description">Invasive Messung des Blutdrucks innerhalb eines Gefäßes.</items>
          <items id="text">Invasiv</items>
        </term_definitions>
        <term_definitions code="at1042">
          <items id="description">Schätzung des durchschnittlichen Blutdrucks über eine 24-stündigen Zeitraum.</items>
          <items id="text">24 Stunden Durchschnitt</items>
        </term_definitions>
        <term_definitions code="at1043">
          <items id="description">Schlafzustand - unterstützt die Auswertung von 24-stündigen ambulanten Aufzeichnungen des Blutdrucks.</items>
          <items id="text">Schlafzustand</items>
        </term_definitions>
        <term_definitions code="at1044">
          <items id="description">Die untersuchte Person ist bei vollem Bewusstsein.</items>
          <items id="text">Aufmerksam und wach</items>
        </term_definitions>
        <term_definitions code="at1045">
          <items id="description">Die untersuchte Person schläft.</items>
          <items id="text">Schlafend</items>
        </term_definitions>
        <term_definitions code="at1051">
          <items id="description">Ein Zeh des Individuums.</items>
          <items id="text">Zeh</items>
        </term_definitions>
        <term_definitions code="at1052">
          <items id="description">Kommentar und Aufzeichung anderer Faktoren die ggf. zu dem Ergebnis der Blutdruckmessung beitragen können. Die kann z.B. bei belastenden Situationen der Fall sein (z.B. sog. Weißkittelhypertonie, Schmerzen, Fieber, Änderungen im atmosphärischen Druck).</items>
          <items id="text">Einflussfaktoren</items>
        </term_definitions>
        <term_definitions code="at1053">
          <items id="description">Invasive Messung mittels Transducer über einen arteriellen Zugang.</items>
          <items id="text">Intraarteriell</items>
        </term_definitions>
        <term_definitions code="at1054">
          <items id="description">Formel, die benutzt wurde, um den systolischen Druck vom mittleren arteriellen Druck zu berechnen (wenn erfasst).</items>
          <items id="text">Formel für systolischen Druck</items>
        </term_definitions>
        <term_definitions code="at1055">
          <items id="description">Formel, die benutzt wurde, um den distolischen Druck vom mittleren arteriellen Druck zu berechnen (wenn erfasst).</items>
          <items id="text">Formel für diastolischen Druck</items>
        </term_definitions>
        <term_definitions code="at1056">
          <items id="description">Fußrücken des Individuums.</items>
          <items id="text">Fußrücken</items>
        </term_definitions>
        <term_definitions code="at1057">
          <items id="description">Strukturierte Körperstelle an der der Blutdruck gemessen wurde.</items>
          <items id="text">Strukturierte Stelle der Messung</items>
        </term_definitions>
        <term_definitions code="at1058">
          <items id="comment">Zum Beispiel: Informationen bzgl. der lokalen Krankenhausabteilung oder zusätzliche Metadata zur Anpassung an entsprechende FHIR oder CIMI Gegenstücke.</items>
          <items id="description">Zusätzliche Information, die für die Erfassung des lokalen Kontexts oder für die Anpassung an andere Referenzmodelle/Formalismen benötigt wird.</items>
          <items id="text">Erweiterung</items>
        </term_definitions>
        <term_definitions code="at1059">
          <items id="description">Einzelnes Wort, Ausdruck oder Kurzbeschreibung der klinischen Bedeutung und Aussagekraft der Blutdruckmessung.</items>
          <items id="text">Klinische Interpretation</items>
        </term_definitions>
