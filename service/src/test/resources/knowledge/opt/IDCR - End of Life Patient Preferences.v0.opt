                        <term_definitions code="at0000">
                            <items id="description">An advance decision to refuse treatment (ADRT) is a decision to refuse a specific treatment, made in
 advance by a person who has capacity to do so.</items>
                            <items id="text">Advance decision to refuse treatment</items>
                        </term_definitions>
                        <term_definitions code="at0001">
                            <items id="description">@ internal @</items>
                            <items id="text">Tree</items>
                        </term_definitions>
                        <term_definitions code="at0002">
                            <items id="description">The date at which the DNACPR decision was originally taken or last reviewed.</items>
                            <items id="text">Date of decision</items>
                        </term_definitions>
                        <term_definitions code="at0003">
                            <items id="description">The state of the decision to refuse treatment.</items>
                            <items id="text">Decision status</items>
                        </term_definitions>
                        <term_definitions code="at0004">
                            <items id="description">The subject has signed an advanced decision to refuse treatment.</items>
                            <items id="text">Advanced decision to refuse treatment signed</items>
                        </term_definitions>
                        <term_definitions code="at0005">
                            <items id="description">The subject has signed an advance decision to refuse treatment (Mental Capacity Act 2005).</items>
                            <items id="text">Has advance decision to refuse treatment (Mental Capacity Act 2005)</items>
                        </term_definitions>
                        <term_definitions code="at0010">
                            <items id="description">@ internal @</items>
                            <items id="text">Tree</items>
                        </term_definitions>
                        <term_definitions code="at0011">
                            <items id="description">The location of the original advanced directive document, either a text description or an electronic link.</items>
                            <items id="text">Location of advance directive documentation</items>
                        </term_definitions>
                        <term_definitions code="at0012">
                            <items id="description">Is the informal carer, or carers, aware of the advanced directive?</items>
                            <items id="text">Informal carer awareness of decision</items>
                        </term_definitions>
                        <term_definitions code="at0013">
                            <items id="description">Has the advanced directive been discussed with a healthcare professional?</items>
                            <items id="text">Discussion with healthcare professional</items>
                        </term_definitions>
                        <term_definitions code="at0021">
                            <items id="description">Other narrative comment pertinent to the advanced directive.</items>
                            <items id="text">Comment</items>
                        </term_definitions>
                        <term_definitions code="at0022">
                            <items id="description">The subject has signed an advance decision to refuse life sustaining treatment (Mental Capacity Act 2005).</items>
                            <items id="text">Has advance decision to refuse life sustaining treatment (Mental Capacity Act 2005)</items>
                        </term_definitions>
                        <term_definitions code="at0025">
                            <items id="description">The informal carer is not aware of the subject's advanced directive.</items>
                            <items id="text">Informal carer not aware of advanced directive</items>
                        </term_definitions>
                        <term_definitions code="at0026">
                            <items id="description">The informal carer is aware of the subject's advanced directive.</items>
                            <items id="text">Informal carer aware of advanced directive</items>
                        </term_definitions>
                        <term_definitions code="at0027">
                            <items id="description">The subject has discussed their decision to refuse treatment with a healthcare professional.</items>
                            <items id="text">Decision to refuse treatment discussed with healthcare professional</items>
                        </term_definitions>
                        <term_definitions code="at0028">
                            <items id="description">The subject has not signed and advance directive or has rescinded an early directive.</items>
                            <items id="text">Advanced directive not signed</items>
                        </term_definitions>
