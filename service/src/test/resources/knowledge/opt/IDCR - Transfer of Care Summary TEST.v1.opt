                              <items id="description">The strength of any diluent used as part of the preparation.</items>
                              <items id="text">Diluent</items>
                            </term_definitions>
                            <term_definitions code="at0124">
                              <items id="comment"> e.g 1, 1.5, 0.125 or 1-2, 12.5-20.5</items>
