                            <items id="text">Baum</items>
                        </term_definitions>
                        <term_definitions code="at0002">
                            <items id="description">Aussage zur Ausstattung des Patienten mit oder zur Implantierung von Medizinprodukten, Stand: "Letzte Aktualisierung".</items>
                            <items id="text">Status</items>
                        </term_definitions>
                        <term_definitions code="at0003">
                            <items id="description">Der Gerätetyp wurde niemals angepasst oder implantiert.</items>
                            <items id="text">Niemals</items>
                        </term_definitions>
                        <term_definitions code="at0004">
                            <items id="description">Der Gerätetyp wird derzeit angepasst oder implantiert.</items>
                            <items id="text">Derzeit</items>
                        </term_definitions>
                        <term_definitions code="at0005">
                            <items id="description">Der Gerätetyp wurde in der Vergangenheit angepasst oder implantiert.</items>
                            <items id="text">Zurückliegend</items>
                        </term_definitions>
                        <term_definitions code="at0007">
                            <items id="description">Namentliche Identifikation des Medizinproduktes.</items>
                            <items id="text">Medizinproduktname</items>
                        </term_definitions>
                        <term_definitions code="at0008">
                            <items id="description">Datum der Anpassung oder der Implantation.</items>
                            <items id="text">Anfangsdatum</items>
                        </term_definitions>
                        <term_definitions code="at0009">
                            <items id="description">Datum, an dem das Medizinprodukt nicht mehr verwendet oder entnommen wurde.</items>
                            <items id="text">Enddatum</items>
                        </term_definitions>
                        <term_definitions code="at0010">
                            <items id="description">Zusätzliche strukturierte Details über das Medizinprodukt.</items>
                            <items id="text">Strukturierte Details</items>
                        </term_definitions>
                        <term_definitions code="at0012">
                            <items id="description">Identifikation der anatomischen Lage bzw. Körperstelle, an der das Gerät angepasst bzw. implantiert wird.</items>
                            <items id="text">Körperstelle</items>
                        </term_definitions>
                        <term_definitions code="at0013">
                            <items id="comment">Gewöhnlich wird dies ein ACTION Archetyp sein, der die Details über die Anpassung oder Implantation enthält.</items>
                            <items id="description">Eine bestimmte anatomische Lage der Körperstelle, an der das Gerät befestigt/implantiert wird.</items>
                            <items id="text">Bestimmte anatomische Lagebezeichnung</items>
                        </term_definitions>
                        <term_definitions code="at0014">
                            <items id="description">Beschreibung des Medizinprodukts.</items>
                            <items id="text">Beschreibung</items>
                        </term_definitions>
                        <term_definitions code="at0015">
                            <items id="description">Beschreibung über die Verwendung des angepassten oder implantierten Medizinproduktes.</items>
                            <items id="text">Beschreibung</items>
                        </term_definitions>
                        <term_definitions code="at0016">
                            <items id="description">@ internal @</items>
                            <items id="text">Baum</items>
                        </term_definitions>
                        <term_definitions code="at0017">
