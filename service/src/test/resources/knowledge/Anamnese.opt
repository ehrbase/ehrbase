                <term_definitions code="at0000">
                    <items id="comment">Die Größe wird bei der Person in stehender Position und die Körperlänge in liegender Position gemessen.</items>
                    <items id="description">Größe bzw. Körperlänge wird vom Scheitel bis zur Fußsohle gemessen.</items>
                    <items id="text">Größe/Länge</items>
                </term_definitions>
                <term_definitions code="at0001">
                    <items id="description">@ internal @</items>
                    <items id="text">History</items>
                </term_definitions>
                <term_definitions code="at0002">
                    <items id="description">Standardwert, ein undefinierter/s Zeitpunkt oder Intervallereignis, das explizit im Template oder zur Laufzeit der Anwendung definiert werden kann.</items>
                    <items id="text">Beliebiges Ereignis</items>
                </term_definitions>
                <term_definitions code="at0003">
                    <items id="description">@ internal @</items>
                    <items id="text">Simple</items>
                </term_definitions>
                <term_definitions code="at0004">
                    <items id="description">Die Länge des Körpers vom Scheitel bis zur Fußsohle.</items>
                    <items id="text">Größe/Länge</items>
                </term_definitions>
                <term_definitions code="at0007">
                    <items id="description">@ internal @</items>
                    <items id="text">List</items>
                </term_definitions>
                <term_definitions code="at0011">
                    <items id="description">Beschreibung des Geräts, das zur Messung der Größe oder Länge verwendet wurde.</items>
                    <items id="text">Gerät</items>
                </term_definitions>
                <term_definitions code="at0013">
                    <items id="description">@ internal @</items>
                    <items id="text">Tree</items>
                </term_definitions>
                <term_definitions code="at0014">
                    <items id="description">Position des Individiums bei der Messung.</items>
                    <items id="text">Position</items>
                </term_definitions>
                <term_definitions code="at0016">
                    <items id="description">Die Größe wird auf beiden Füßen stehend gemessen, wobei das Gewicht gleichmäßig verteilt ist, die Fersen zusammen und sowohl das Gesäß als auch die Fersen in Kontakt mit einem vertikalen Rückenbrett.</items>
                    <items id="text">Stehend</items>
                </term_definitions>
                <term_definitions code="at0018">
                    <items id="description">Zusätzliche Darstellung der Messung, die in anderen Bereichen nicht erfasst wurde.</items>
                    <items id="text">Kommentar</items>
                </term_definitions>
                <term_definitions code="at0019">
                    <items id="comment">Zum Beispiel: Feststellung einer Amputation.</items>
                    <items id="description">Beschreibung aller Probleme oder Faktoren, die sich auf die Messung auswirken können.</items>
                    <items id="text">Einflussfaktoren</items>
                </term_definitions>
                <term_definitions code="at0020">
                    <items id="description">Länge wird in einer voll ausgestreckten, liegenden Position gemessen. Hierbei wird das Becken flach gehalten, die Beine ausgestreckt und die Füße gebeugt.</items>
                    <items id="text">Liegend</items>
                </term_definitions>
                <term_definitions code="at0021">
                    <items id="description">Normalerweise wird die erste Längenmessung kurz nach der Geburt aufgezeichnet. Dieses Ereignis wird nur einmal pro Gesundheitsakte verwendet.</items>
                    <items id="text">Geburt</items>
                </term_definitions>
                <term_definitions code="at0022">
