                                <term_definitions code="at0000">
                                  <items id="description">Zur Einordnung eines Arzneimittels in eine Gruppe, Klasse oder Kategorie.</items>
                                  <items id="text">Arzneimittelgruppe</items>
                                </term_definitions>
                                <term_definitions code="at0001">
                                  <items id="description">Name der Arzneimittelgruppe.</items>
                                  <items id="text">Name</items>
                                </term_definitions>
                                <term_definitions code="at0002">
                                  <items id="comment">Bekannte Arzneimittelverzeichnisse in Deutschland sind u.a. die Rote Liste, die Gelbe Liste oder die Lauer-Taxe.</items>
                                  <items id="description">Name des verwendeten Katalogs oder Verzeichnisses.</items>
                                  <items id="text">Katalogname</items>
                                </term_definitions>
                                <term_definitions code="at0004">
                                  <items id="description">Hier können Arzneimittelverzeichnisse, sowie intern verwendete Kataloge angegeben werden.</items>
                                  <items id="text">Katalog</items>
                                </term_definitions>
                                <term_definitions code="at0005">
                                  <items id="description">Versionsnummer des verwendeten Katalogs oder Verzeichnisses.</items>
                                  <items id="text">Version</items>
                                </term_definitions>
                                <term_definitions code="at0007">
