                        <archetype_id>
                            <value>openEHR-EHR-OBSERVATION.medication_statement.v0</value>
                        </archetype_id>
                        <term_definitions code="at0000">
                            <items id="text">Medication statement</items>
                            <items id="description">A snapshot view about the use of a specified medication.</items>
                        </term_definitions>
                        <term_definitions code="at0001">
                            <items id="text">History</items>
                            <items id="description">@ internal @</items>
                        </term_definitions>
                        <term_definitions code="at0002">
                            <items id="text">Any event</items>
                            <items id="description">Default, unspecified point in time or interval event which may be explicitly defined in a template or at run-time.</items>
                        </term_definitions>
                        <term_definitions code="at0003">
                            <items id="text">Tree</items>
                            <items id="description">@ internal @</items>
                        </term_definitions>
                        <term_definitions code="at0004">
                            <items id="text">Item tree</items>
                            <items id="description">@ internal @</items>
                        </term_definitions>
                        <term_definitions code="at0005">
                            <items id="text">Extension</items>
                            <items id="description">Additional information required to extend the model with local content or to align with other reference models or formalisms.</items>
                            <items id="comment">For example: local information requirements; or additional metadata to align with FHIR.</items>
                        </term_definitions>
                        <term_definitions code="at0006">
                            <items id="text">Medication name</items>
                            <items id="description">Name of the medication, vaccine or other therapeutic/prescribable item.</items>
                            <items id="comment">It is strongly recommended that the 'Medication item' be coded with a terminology capable of triggering decision support, where possible. The extent of coding may vary from the simple generic or product name of the medication item through to structured details about the actual medication pack to be used. Free text entry should only be used if there is no appropriate terminology available.</items>
                        </term_definitions>
                        <term_definitions code="at0008">
                            <items id="text">Status</items>
                            <items id="description">The status of use of the medication.</items>
                            <items id="comment">For example: the medication is still actively being taken; or a course of antibiotics has been completed.</items>
                        </term_definitions>
                        <term_definitions code="at0009">
                            <items id="text">Planned</items>
                            <items id="description">The medication is intended to be taken at an unspecified time in the future.</items>
                        </term_definitions>
                        <term_definitions code="at0010">
                            <items id="text">Scheduled</items>
                            <items id="description">The medication is scheduled to be taken at a future date.</items>
                        </term_definitions>
                        <term_definitions code="at0011">
                            <items id="text">Active</items>
                            <items id="description">The medication is currently being taken.</items>
                        </term_definitions>
                        <term_definitions code="at0012">
                            <items id="text">Completed</items>
                            <items id="description">The medication is no longer being taken.</items>
                        </term_definitions>
                        <term_definitions code="at0013">
                            <items id="text">Postponed</items>
                            <items id="description">The medication is planned to be taken but commencement has been temporarily delayed.</items>
                        </term_definitions>
                        <term_definitions code="at0014">
                            <items id="text">Cancelled</items>
                            <items id="description">The medication was planned to be taken but cancelled before commencement.</items>
                        </term_definitions>
                        <term_definitions code="at0015">
                            <items id="text">Suspended</items>
                            <items id="description">The medication was commenced but use temporarily supended.</items>
                        </term_definitions>
                        <term_definitions code="at0016">
                            <items id="text">Discontinued</items>
                            <items id="description">The medication was commenced but use was ceased.</items>
                        </term_definitions>
                        <term_definitions code="at0019">
                            <items id="text">Start date</items>
                            <items id="description">The valid start date/time for the Status.</items>
                        </term_definitions>
                        <term_definitions code="at0020">
                            <items id="text">Status duration</items>
                            <items id="description">The valid duration for the Status.</items>
                            <items id="comment">This data element can be combined with either the Status start time or the Status end time, but not both.</items>
                        </term_definitions>
                        <term_definitions code="at0021">
                            <items id="text">End date</items>
                            <items id="description">The valid end date/time for the Status.</items>
                        </term_definitions>
                        <term_definitions code="at0023">
                            <items id="text">Clinical indication</items>
                            <items id="description">The clinical reason for use of the medication item.</items>
                            <items id="comment">For example: 'Angina'. Coding of the clinical indication with a terminology is preferred, where possible. This data element allows multiple occurrences.</items>
                        </term_definitions>
                        <term_definitions code="at0029">
                            <items id="text">Comment</items>
                            <items id="description">Additional narrative about the medication statement not captured in other fields.</items>
                        </term_definitions>
                        <term_definitions code="at0007">
                            <items id="text">Preparation details</items>
                            <items id="description">Structured details about the overall preparation including strength, form and constituent substances.</items>
                            <items id="comment">Use this SLOT where the detailed description of the ordered item needs to be explicitly stated. For example: the form, strength, any diluents or mixture of ingredients.</items>
                        </term_definitions>
                        <term_definitions code="at0022">
                            <items id="text">Structured dose and timing directions</items>
                            <items id="description">Details of structured dose and timing directions.</items>
                        </term_definitions>
                        <term_definitions code="at0018">
                            <items id="text">Status reason</items>
                            <items id="description">Reason for the status.</items>
                            <items id="comment">For example: abandoned due to adverse reaction; suspended due to pending surgery.</items>
                        </term_definitions>
                        <term_definitions code="at0024">
                            <items id="text">First prescribed</items>
                            <items id="description">The date/time when a prescription for the medication was first issued.</items>
                        </term_definitions>
                        <term_definitions code="at0025">
                            <items id="text">Last prescribed</items>
                            <items id="description">The date/time when a prescription for the medication was last issued.</items>
                        </term_definitions>
                        <term_definitions code="at0026">
                            <items id="text">Last administered</items>
                            <items id="description">The date/time when the medication was last taken by or administered to the individual.</items>
                        </term_definitions>
                        <term_definitions code="at0027">
                            <items id="text">Last reviewed</items>
                            <items id="description">The date/time when usage of the medication was last reviewed by a clinician or phamacist.</items>
                        </term_definitions>
                        <term_definitions code="at0028">
                            <items id="text">Discontinued</items>
                            <items id="description">The date/time when use of the medication was ceased.</items>
                        </term_definitions>
                        <term_definitions code="at0017">
                            <items id="text">Unknown</items>
                            <items id="description">The status of the medication is not known.</items>
                        </term_definitions>
                        <term_definitions code="at0030">
                            <items id="text">Route</items>
                            <items id="description">The route by of administration of the medication into the body.</items>
                            <items id="comment">For example: 'oral', 'intravenous', or 'topical'. Coding of the route with a terminology is preferred, where possible. Multiple potential routes may be specified if the medication is planned or scheduled; only one is relevant if the medication has been administered.</items>
                        </term_definitions>
                        <term_definitions code="at0031">
                            <items id="text">Therapeutic intent</items>
                            <items id="description"></items>
                        </term_definitions>
                    </children>
                    <children xsi:type="C_ARCHETYPE_ROOT">
                        <rm_type_name>EVALUATION</rm_type_name>
                        <occurrences>
