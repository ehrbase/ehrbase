                        </term_definitions>
                        <term_definitions code="at0010">
                            <items id="text">Entièrement habillé, y compris les chaussures</items>
                            <items id="description">Les vêtements qui peuvent augmenter considérablement le poids, y compris les chaussures.</items>
                        </term_definitions>
                        <term_definitions code="at0011">
                            <items id="text">Légèrement vêtu/en sous-vêtement</items>
                            <items id="description">Des vêtements qui n'augmentent pas le poids de manière significative.</items>
                        </term_definitions>
