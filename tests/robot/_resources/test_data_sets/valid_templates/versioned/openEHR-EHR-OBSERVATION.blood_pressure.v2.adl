						}
					}
					ELEMENT[at0014] occurrences matches {0..1} matches {	-- Location of measurement
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0025, 	-- Right arm
									at0026, 	-- Left arm
									at0027, 	-- Right thigh
									at0028, 	-- Left thigh
									at1020, 	-- Right wrist
									at1021, 	-- Left wrist
									at1026, 	-- Right ankle
									at1031, 	-- Left ankle
									at1032, 	-- Finger
									at1051, 	-- Toe
